	<set name="com.macrabbit.css.property.flood-color">
		<completion string="inherit"/>
		<!--@color-functions-->
		<completion string="rgb">
			<behavior>
				<append-dynamic>
					<matched-suffix>(\()|[^(]</matched-suffix>
					<transform-into>(?1::(${1:255}, ${2:255}, ${3:255}\))</transform-into>
				</append-dynamic>
			</behavior>
		</completion>
	</set>
	
